{
    "version": 2,
    "name": "RupeeTrack-Server",
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js",
            "methods": ["GET", "POST"]
        }
    ]
}
